    <string name="action_help">Ayuda</string>
    <string name="welcome_text">Privacy friendly QR scanner le permite escanear códigos de barras y códigos QR con diferentes tipos de contenido, requiriendo de un conjunto mínimo de permisos y ayudando en la detección de enlaces maliciosos.</string>
